{
  "dashboard": {
    "id": null,
    "title": "California Housing API Monitoring",
    "tags": ["housing", "api", "mlops"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "API Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(housing_api_prediction_requests_total[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "housing_api_error_rate_percentage",
            "legendFormat": "Error Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 5
                },
                {
                  "color": "red",
                  "value": 10
                }
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Average Processing Time",
        "type": "stat",
        "targets": [
          {
            "expr": "housing_api_average_processing_time_seconds",
            "legendFormat": "Avg Time"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Predictions Last 24h",
        "type": "stat",
        "targets": [
          {
            "expr": "housing_api_predictions_last_24h",
            "legendFormat": "Total Predictions"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Request Rate Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(housing_api_prediction_requests_total[1m])",
            "legendFormat": "{{endpoint}} - {{method}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Processing Time Distribution",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(housing_api_prediction_processing_time_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(housing_api_prediction_processing_time_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(housing_api_prediction_processing_time_seconds_bucket[5m]))",
            "legendFormat": "99th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 7,
        "title": "Prediction Value Distribution",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(housing_api_prediction_values_dollars_bucket[5m]))",
            "legendFormat": "Median Price"
          },
          {
            "expr": "histogram_quantile(0.75, rate(housing_api_prediction_values_dollars_bucket[5m]))",
            "legendFormat": "75th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(housing_api_prediction_values_dollars_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "currencyUSD"
          }
        },
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 17
        }
      },
      {
        "id": 8,
        "title": "HTTP Status Code Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (status_code) (housing_api_prediction_requests_total)",
            "legendFormat": "{{status_code}}"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 17
        }
      },
      {
        "id": 9,
        "title": "MLflow Metrics",
        "type": "table",
        "targets": [
          {
            "expr": "housing_api_mlflow_experiments_total",
            "legendFormat": "Experiments",
            "format": "table"
          },
          {
            "expr": "housing_api_mlflow_runs_total", 
            "legendFormat": "Runs",
            "format": "table"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 26
        }
      },
      {
        "id": 10,
        "title": "Database Size",
        "type": "stat",
        "targets": [
          {
            "expr": "housing_api_database_size_bytes",
            "legendFormat": "DB Size"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "bytes"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 26
        }
      },
      {
        "id": 11,
        "title": "Average Prediction Value",
        "type": "stat",
        "targets": [
          {
            "expr": "housing_api_average_prediction_value_dollars",
            "legendFormat": "Avg Price"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "currencyUSD"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 26
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "10s",
    "schemaVersion": 27,
    "version": 1,
    "gnetId": null
  }
}
