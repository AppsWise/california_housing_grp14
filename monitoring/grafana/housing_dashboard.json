{
  "dashboard": {
    "id": null,
    "title": "California Housing MLOps Dashboard",
    "tags": ["mlops", "housing", "predictions", "machine-learning"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Prediction Volume",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(housing_api_predictions_total[5m]) * 60",
            "legendFormat": "Predictions/min",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Average Prediction Value",
        "type": "stat",
        "targets": [
          {
            "expr": "housing_api_avg_prediction_value_dollars{time_window=\"24h\"}",
            "legendFormat": "Avg Price (24h)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "decimals": 0,
            "color": {
              "mode": "value"
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Processing Time",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(housing_api_processing_time_seconds_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 3,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.1},
                {"color": "red", "value": 0.5}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "housing_api_error_rate_percent{time_window=\"1h\"}",
            "legendFormat": "Error Rate (1h)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Predictions Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(housing_api_predictions_total[5m])",
            "legendFormat": "{{endpoint}} - {{status}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Predictions/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Prediction Values Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "housing_api_prediction_value_dollars",
            "legendFormat": "{{longitude_range}}-{{latitude_range}} ({{income_range}} income)",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Price ($)",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "Processing Time Distribution",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(housing_api_processing_time_seconds_bucket[5m])",
            "legendFormat": "{{le}}",
            "refId": "A"
          }
        ],
        "heatmap": {
          "xBucketSize": null,
          "yBucketSize": null
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "Geographic Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "housing_api_predictions_by_region",
            "legendFormat": "{{region}}-{{state}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 9,
        "title": "MLflow Experiments",
        "type": "table",
        "targets": [
          {
            "expr": "housing_api_mlflow_runs_total",
            "legendFormat": "{{experiment_name}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 10,
        "title": "Database Size",
        "type": "graph",
        "targets": [
          {
            "expr": "housing_api_database_size_bytes",
            "legendFormat": "Database Size",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Bytes",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 11,
        "title": "Prediction Rate by Time Window",
        "type": "graph",
        "targets": [
          {
            "expr": "housing_api_prediction_rate_per_hour",
            "legendFormat": "{{time_window}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Predictions/hour",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32}
      }
    ]
  }
}
